class Solution {
  public:
    // Function to return the count of number of elements in union of two arrays.
    int findUnion(vector<int>& a, vector<int>& b) {
        // code here
        set<int> c;
        for(int n:a){
            c.insert(n);
        }
        for(int n:b){
            c.insert(n);
        }
        return c.size();
    }
};
